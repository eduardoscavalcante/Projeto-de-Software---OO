package classes;

import java.util.Scanner;

public class Gerente extends Funcionarios{
	Scanner input = new Scanner(System.in);

    private int qntHoras;
	
	public Gerente(int id, String nome, String email, long telefone, long cpf, int cargo, int qntHoras) {
		super(id, nome, email, telefone, cpf, cargo, qntHoras);
		this.qntHoras = qntHoras;
	}

    public int getQntHoras() {
        return qntHoras;
    }
}
